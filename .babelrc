{
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-flow-strip-types",
    "transform-class-properties",
    "react-css-modules"
  ],
  "presets": [
    "es2015",
    "stage-0",
    "stage-2",
    "react",
    "flow",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "modules": false
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
